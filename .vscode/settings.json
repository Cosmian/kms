{
  "cSpell.words": [
    "canonicalize",
    "certbot",
    "Certbot",
    "chacha",
    "ciphertext",
    "ciphertexts",
    "ckms",
    "cleartext",
    "cloudproof",
    "cosmian",
    "Cosmian",
    "Covercrypt",
    "ECDH",
    "ecies",
    "ECIES",
    "gpsw",
    "Gpsw",
    "jwks",
    "keypair",
    "kmip",
    "KMIP",
    "mysql",
    "pkcs",
    "postgres",
    "postgresql",
    "Seedable",
    "sqlcipher",
    "sqlite",
    "Sqlite",
    "structopt",
    "TFHE",
    "ttlv",
    "uids",
    "Upserting",
    "uuid",
    "Uuid"
  ],
  "[md]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.formatOnSave": false,
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.formatOnSave": false
  },
  "python.analysis.typeCheckingMode": "basic",
  "prettier.proseWrap": "preserve",
  "grammarly.files.include": [
    "**/readme.md",
    "**/README.md",
    "**/*.txt",
    "**/*.md"
  ],
  "rust-analyzer.check.command": "clippy",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.provider": "none"
}
